.main{
	/* width: 100%; */
	padding: 0 49rpx;
	font-size: 30rpx;
	color: #c2c2c2;
}
.head {
	margin-top: 180rpx;
	margin-bottom: 120rpx;
	/* width: 100%; */
	display: flex;
	justify-content: center;

}
.head image {
	width: 119rpx;
	height: 119rpx;
}
.w .row ,.w .row1{
	 /* padding: 0 85rpx; */
	width: 550rpx;
	margin-left: 50rpx;
  /* margin: 0 auto; */
	display: flex;
	padding: 20rpx 0;
	border-bottom: 1rpx solid rgba(238,238,238,1);
}
.w .row1 {
	justify-content: space-between;
}
.w .row .left {
	display: flex;
	align-items: center;
	border-right: 1rpx solid rgba(238,238,238,1);
	margin-right: 20rpx;
}
.w .row .left text {
	font-size:24rpx;
font-family:Source Han Sans CN;
font-weight:400;
color:rgba(51,51,51,1);
}
.w .row .left image {
	width: 18rpx;
	height: 11rpx;
	margin-left: 18rpx;
	margin-right: 15rpx;
}
.w .row1 .right{
	width: 120rpx;
	/* margin-left: 20rpx; */
	margin-right: 20rpx;
  margin-top: 7rpx;
	font-size:24rpx;
	font-family:Source Han Sans CN;
	font-weight:400;
	color:rgba(115,168,247,1);
}
.foot .login_btn {
	width:650rpx;
height:70rpx;
background:linear-gradient(0deg,rgba(177,206,250,1) 0%,rgba(208,226,252,1) 100%);
