/**index.wxss**/
.main {
	font-size: 35rpx;
	color: #333333;
	background: #f8f8f8;
	height: 100vh;
	width: 100vw;
	/* background: #000; */
	/* padding: 20rpx 30rpx; */
}

.page {
	/* margin: 20rpx 30rpx; */
	background: #fff;
	border-radius: 20rpx;
	position: absolute;
	width: 690rpx;
	margin: 0 auto;
	margin-left: 25rpx;
	margin-top: 40rpx;
}

.info {
	display: flex;
}

.info-item {
	text-align: center;
	flex: 1;
	padding: 30rpx 0;
}

.info-title {
	font-size: 40rpx;
	color: #333333;
}

.info-no {
	font-size: 20rpx;
	color: #999999;
	margin-top: 20rpx;
}

.queue {
	margin: 10rpx 30rpx 0;
	text-align: center;
	color: #28b9db;
	padding-bottom: 30rpx;
	font-size: 22rpx;
	border-bottom: 2rpx solid #EEEEEE;
}

.service {
	overflow: hidden;
	margin: 0 20rpx;
	padding: 30rpx;
	font-size: 24rpx;
	border-bottom: 2rpx solid #EEEEEE;
}

.sevname {
	float: left;
}

.sevprice {
	float: right;
	color: #FF5151;
}

.sevprice text {
	font-size: 25rpx;
}

.msg {
	overflow: hidden;
	margin: 0 30rpx;
	padding: 30rpx;
	text-align: center;
	border-bottom: 2rpx solid #EEEEEE;
}

.notice {
	font-size: 28rpx;
	font-family: Source Han Sans CN;
	font-weight: 500;
	color: rgba(51, 51, 51, 1);
}

.jianyi {
	font-size: 24rpx;
	font-family: Source Han Sans CN;
	font-weight: 400;
	color: rgba(51, 51, 51, 1);
	line-height: 36rpx;

}

.msg text {
	color: #28b9db;
}

.jianyi {
	font-size: 24rpx;
	margin-top: 20rpx;
}

.paijiv {
	width: 680rpx;
	text-align: center;
	line-height: 80rpx;
	color: white;
	background: linear-gradient(0deg, #156df2 0%, #2a7fff 100%);
	font-size: 30rpx;
	border-radius: 40rpx;
	position: absolute;
	bottom: 180rpx;
	left: 36rpx;
}

.xuzhi {
	padding: 30rpx;
}

.rule {
	color: #b4b4b4;
	font-size: 24rpx;
	padding-left: 20rpx;
}

.rulerow {
	font-size: 24rpx;
	margin-top: 30rpx;
	color: #999999;
	padding: 0 32rpx;
}

.ruleone {
	font-size: 24rpx;
	margin-top: 15rpx;
	color: #156df2;
	margin-top: 19rpx;
}

.zhezhao {
	position: fixed;
	top: 0;
	left: 0;
	width: 100%;
	height: 100%;
	background: rgba(0, 0, 0, .5);

	z-index: 999;
	display: flex;
	align-items: center;
	justify-content: center
}

.pos {
	position: fixed;
	width: 620rpx;
	left: 50%;
	margin-left: -310rpx;
	top: 180rpx;
	background: #fff;
	border-radius: 15rpx;
	color: #333333;
}

.header {
	text-align: center;
	font-size: 35rpx;
	padding: 25rpx 0;
	border-bottom: 1rpx solid #e6e6e6;
}

.message {
	padding: 30rpx;
	display: flex;
	border-bottom: 1rpx solid #e6e6e6;
}

.messicon {
	width: 150rpx;
}

.messicon image {
	width: 150rpx;
	height: 150rpx;
	border-radius: 50%;
}

.messcontent {
	margin-left: 30rpx;
}

.messname {
	margin-top: 30rpx;
	color: #333;
	font-size: 35rpx;
}

.messtext {
	margin-top: 5rpx;
	font-size: 28rpx;
}

.going {
	font-size: 35rpx;
	padding: 0 40rpx 30rpx;
}

.gorow {
	display: flex;
	margin-top: 30rpx;
}

.gotext {
	flex: 1;
}

.goimg {
	margin: 0 15rpx;
}

.gotime {
	width: 180rpx;
}

.goimg image {
	width: 30rpx;
	height: 35rpx;
	margin-top: 8rpx;
}

.know {
	margin-top: 25rpx;
	padding: 25rpx 0;
	color: #61cce5;
	text-align: center;
	border-top: 1rpx solid #e6e6e6;
}

.zhezhaoBox {
	width: 85%;
	background: #fff;
	border-radius: 10rpx;
}

.title {
	font-size: 30rpx;
	text-align: center;
	padding: 20rpx 0;
	border-bottom: 1rpx solid #e6e6e6
}

.myNow {
	height: 90rpx;
	background: linear-gradient(0deg, #2a7fff 0%, #156df2 100%);
	line-height: 90rpx;
	text-align: center;
	color: #fff;
	font-size: 30rpx;
	margin-top: 50rpx
}

.footer_ {
	width: 100%;
	text-align: center;
	font-size: 24rpx;
	font-family: Source Han Sans CN;
	font-weight: 400;
	color: rgba(21, 109, 242, 1);
	position: absolute;
	bottom: 120rpx;
}