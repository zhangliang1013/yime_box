/* pages/shouye/lingdu/lingdu.wxss */
.main {
  background: #ffffff;
  /* padding:0 20rpx; */
}

.commonHead_left {
  width: 125rpx;
  height: 175rpx;
  position: absolute;
  left: 0;
  top: 0;
  z-index: 999;
}

.commonHead_left_back {
  width: 100%;
  height: 100%;
  background-image: url(data:image/png;base64,);
  background-size: 50rpx 50rpx;
  background-repeat: no-repeat;
  background-position: center;
}

.contain {
  /* width:100%; */
  height: 280rpx;
  background: linear-gradient(0deg, rgba(10, 101, 241, 1) 23%, rgba(40, 74, 242, 1) 100%);
  /* border-radius: 0 0 30rpx 30rpx; */
  padding-left: 36rpx;
  /* margin-top: 100rpx; */
  padding-top: 150rpx;
  display: flex;
  position: relative;
  overflow: hidden;
}
.contain::after{
  content: "";
  position: absolute;
  bottom: -40rpx;
  left: 0;
  width: 100vw;
  height:100rpx;
  background-color: #fff;
  border-radius: 150% 150% 0 0;
}

.contain image {
  height: 160rpx;
  width: 160rpx;
  border-radius: 20rpx;
}

.contain .yinye {
  /* font-size: 15px; */
  margin-left: 20rpx;
  margin-top: 8rpx;
  font-size: 20rpx;
  font-family: Source Han Sans CN;
  font-weight: 400;
  color: rgba(255, 255, 255, 1);
}

.contain .yinye view {
  margin-bottom: 30rpx;
}

.jieshao {
  /* margin-top: 30rpx; */
  padding: 0 34rpx;
  background: rgba(255, 255, 255, 1);
  font-size: 22rpx;
  font-family: Source Han Sans CN;
  font-weight: 400;
  color: rgba(51, 51, 51, 1);
  line-height: 36rpx;
}
.jie_title {
  font-size:28rpx;
font-family:Source Han Sans CN;
font-weight:bold;
color:rgba(51,51,51,1);
line-height:30rpx;
width: 100%;
text-align: center;
margin-bottom: 30rpx;
}

.title {
  margin: 50rpx 0;

  margin-top: 60rpx;
  margin-bottom: 20rpx;
  font-size: 28rpx;
  font-family: Source Han Sans CN;
  font-weight: bold;
  color: rgba(51, 51, 51, 1);
  /* line-height:30px; */
}

.banxin {
  padding: 0 35rpx;
}

.menzhen_title {
  margin-top: 50rpx;
}

.menzhen_content {
  padding: 0 30rpx;
  padding-bottom: 30rpx;
  /* border: 1px solid #ccc; */
  box-shadow:3rpx 4rpx 21rpx 3rpx rgba(21,109,242,0.29);
  border-radius: 20rpx;
  background: rgba(255, 255, 255, 1);
  border-radius: 20px;
}

.one {
  border-bottom: 1rpx solid #eee;
  padding: 30rpx 0;
}
.one1{
  border-bottom : none;
}

.one text {
  font-size: 24rpx;
  font-family: Source Han Sans CN;
  font-weight: bold;
  color: rgba(51, 51, 51, 1);
  /* line-height:30rpx; */
}

.one image {
  width: 14rpx;
  height: 26rpx;
}

.one .xia {
  width: 26rpx;
  height: 14rpx;
}

.one .one_title {
  width: 100%;
  display: flex;
  justify-content: space-between;
  /* margin-bottom: 30rpx; */
}

.xiangxi {
  height: 83rpx;
  font-size: 24rpx;
  font-family: Source Han Sans CN;
  font-weight: 400;
  color: rgba(51, 51, 51, 1);
  line-height: 30rpx;
  margin-top: 30rpx;
}

.zhunajia_list {
  display: flex;
  justify-content: space-around;
}

.zhunajia_list .list_item {
  display: flex;
  flex-direction: column;
  align-items: center;
}

.zhunajia_list .list_item image {
  width: 148rpx;
  height: 148rpx;
}

.zhunajia_list .list_item .name {
  font-size: 24rpx;
  font-family: Source Han Sans CN;
  font-weight: 500;
  color: rgba(50, 51, 51, 1);
  margin-top: 20rpx;
  margin-bottom: 10rpx;
}

.zhunajia_list .list_item .zhiwei {
  font-size: 18rpx;
  font-family: Source Han Sans CN;
  font-weight: 400;
  color: rgba(50, 51, 51, 1);
}

.ren_list {
  display: flex;
  /* justify-content: space-around; */
  flex-wrap: wrap;
}

.ren_list image {
  width: 152rpx;
  height: 122rpx;
}

.ren_list view {
  margin-bottom: 20rpx;
  margin-right: 23rpx;
}
.ren_list view:nth-child(4n){
   margin-right: 0;
}
.title_name {
  position: absolute;
  height: 175rpx;
  line-height: 175rpx;
  z-index: 999;
  left: 320rpx;
  top: 0;
  font-size: 32rpx;
  font-family: Source Han Sans CN;
  font-weight: 400;
  color: rgba(255, 255, 255, 1);
   margin: 0 auto;
}

