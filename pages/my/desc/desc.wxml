<!-- 个人档案页面(疼痛位置)-->
<view class="main">
	<view class="topimg">
		<view class="schedule1"></view>
		<view class="schedule2"></view>
		<view class="schedule3"></view>
		<view class="schedule4"></view>
		<view class="schedule5"><text style="font-size: 24rpx; font-weight: 500;color: #FFAB50;">领取红包</text></view>
	</view>
	<view class="content">
		<view class="chead">疼痛位置</view>
		<view class="row">
			<view class="item {{activeindex == index?'active':''}}" wx:for="{{a}}" data-item="{{item}}" data-index="{{index}}" wx:key='index' bindtap="selit">
			{{item}}</view>
		</view>
	</view>
  	<view class="next" bindtap="next">下一步</view>
	<view class="fixed">
			<view>如有更多健康需要，可点击</view>
		<view><button open-type="contact">医么客服</button></view>
	</view>
</view>
